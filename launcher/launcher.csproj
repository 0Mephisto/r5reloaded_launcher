<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows7.0</TargetFramework>
	<Nullable>disable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
	<ApplicationIcon>launcher_x64.ico</ApplicationIcon>
	<IncludeAllContentForSelfExtract>true</IncludeAllContentForSelfExtract>
	<NoWarn>$(NoWarn);SYSLIB0014;IDE1006;IDE0130;CA1835;SYSLIB1054;SYSLIB1045;CA1416;CS4014;CA1510;IDE0044</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="assets\icons8-check-48.png" />
    <None Remove="assets\icons8-download-96.png" />
    <None Remove="assets\icons8-info-96.png" />
    <None Remove="assets\icons8-left-arrow-100.png" />
    <None Remove="assets\icons8-link-100.png" />
    <None Remove="assets\icons8-linking-48.png" />
    <None Remove="assets\icons8-settings-100.png" />
    <None Remove="assets\icons8-x-50.png" />
    <None Remove="assets\launcher_bg.jpg" />
    <None Remove="assets\launcher_bg_autum.mp4" />
    <None Remove="assets\launcher_bg_beach.mp4" />
    <None Remove="assets\launcher_bg_kc.mp4" />
    <None Remove="assets\launcher_bg_shadow.mp4" />
    <None Remove="assets\launcher_bg_we.mp4" />
    <None Remove="assets\launcher_x64.ico" />
    <None Remove="assets\logo-50.png" />
    <None Remove="assets\logo-main-white.png" />
    <None Remove="assets\logo-main.png" />
    <None Remove="assets\news_default_bg.jpg" />
    <None Remove="assets\preload.gif" />
    <None Remove="assets\R5R-Logo-White.gif" />
    <None Remove="assets\Revenant-apex.jpg" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Hardcodet.NotifyIcon.Wpf" Version="2.0.1" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
    <PackageReference Include="Octopus.Octodiff" Version="2.0.547" />
    <PackageReference Include="Polly" Version="8.5.1" />
    <PackageReference Include="SoftCircuits.IniFileParser" Version="2.6.0" />
    <PackageReference Include="System.Net.Http.WinHttpHandler" Version="9.0.1" />
    <PackageReference Include="ValveKeyValue" Version="0.12.0.391" />
    <PackageReference Include="WindowsAPICodePack" Version="8.0.6" />
    <PackageReference Include="XamlAnimatedGif" Version="2.3.0" />
    <PackageReference Include="ZstdSharp.Port" Version="0.8.4" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="assets\launcher_bg_autum.mp4">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="assets\launcher_bg_beach.mp4">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Content>
    <Content Include="assets\launcher_bg_kc.mp4">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Content>
    <Content Include="assets\launcher_bg_shadow.mp4">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Content>
    <Content Include="assets\launcher_bg_we.mp4">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Content>
    <None Include="..\.editorconfig" Link=".editorconfig" />
    <Resource Include="assets\icons8-check-48.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="assets\icons8-download-96.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="assets\icons8-info-96.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="assets\icons8-left-arrow-100.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="assets\icons8-link-100.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="assets\icons8-linking-48.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="assets\icons8-x-50.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="assets\launcher_bg.jpg">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="assets\logo-50.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="assets\news_default_bg.jpg">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="assets\Revenant-apex.jpg">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="launcher_x64.ico">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="assets\icons8-settings-100.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="assets\logo-main-white.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="assets\logo-main.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="assets\R5R-Logo-White.gif">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Controls\Items\NewsItemSmall.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="Controls\Menus\Pages\AdvancedSettings\NetworkSettings.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="Controls\Menus\Pages\AdvancedSettings\GeneralSettings.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="Controls\Popups\OnBoardAskPopup.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="Controls\Popups\OnBoardPopup.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="Controls\Popups\CheckExisitngFilesPopup.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="Controls\Menus\AdvancedControl.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="Controls\Popups\AskToQuitPopup.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="Controls\Popups\InstallLocation.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <Page Update="Controls\Popups\OnBoardPopup.xaml">
      <SubType>Designer</SubType>
    </Page>
    <Page Update="Controls\Popups\CheckExisitngFilesPopup.xaml">
      <SubType>Designer</SubType>
    </Page>
    <Page Update="MainWindow.xaml">
      <CopyToOutputDirectory></CopyToOutputDirectory>
      <SubType>Designer</SubType>
    </Page>
  </ItemGroup>

</Project>
